{
  "name": "data-sync-ingestion",
  "version": "1.0.0",
  "description": "DataSync API Ingestion Challenge",
  "main": "packages/ingestion.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/packages/ingestion.js",
    "dev": "cross-env DB_HOST=localhost DB_PORT=5434 DB_NAME=postgres DB_USER=postgres DB_PASSWORD=postgres ts-node packages/ingestion.ts",
    "dev:local": "set DB_HOST=localhost&& set DB_PORT=5434&& set DB_NAME=postgres&& set DB_USER=postgres&& set DB_PASSWORD=postgres&& ts-node packages/ingestion.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "license": "ISC",
