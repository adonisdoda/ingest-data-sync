    "node-fetch": "^2.7.0",
    "pg": "^8.18.0",
    "@types/node": "^20.11.0",
    "@types/node-fetch": "^2.6.11",
    "@types/pg": "^8.16.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.111.0",
    "@types/node-fetch": "^2.6.11",
    "@types/pg": "^8.16.0",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
